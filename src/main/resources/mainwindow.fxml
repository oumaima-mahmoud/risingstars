<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Label?>

<VBox spacing="20" style="-fx-padding: 40;" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainController">
    <Label text="Gestion des Applications" style="-fx-font-size: 24px; -fx-font-weight: bold;" />

    <Button text="Gestion des Réclamations" style="-fx-background-color: #007bff; -fx-text-fill: white; -fx-font-size: 16px;" onAction="#openGestionReclamation"/>

    <Button text="Gestion des Offres" style="-fx-background-color: #28a745; -fx-text-fill: white; -fx-font-size: 16px;" onAction="#openOffre"/>

    <Button text="Gestion des Publicités" style="-fx-background-color: #dc3545; -fx-text-fill: white; -fx-font-size: 16px;" onAction="#openPublicite"/>

    <Button text="Gestion des Paniers" style="-fx-background-color: #007bff; -fx-text-fill: white; -fx-font-size: 16px;" onAction="#openGestionPanier"/>

    <Button text="Gestion des Commandes" style="-fx-background-color: #007bff; -fx-text-fill: white; -fx-font-size: 16px;" onAction="#openGestionCommande"/>
</VBox>
